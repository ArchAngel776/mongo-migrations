{
    "assumptions": {
        "setPublicClassFields": true,
        "noClassCalls": true,
        "setClassMethods": true
    },
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        "babel-plugin-typescript-path-aliases",
        "@babel/plugin-transform-class-properties",
        "@babel/plugin-transform-classes"
    ]
}
